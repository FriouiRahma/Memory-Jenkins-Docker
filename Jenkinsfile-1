pipeline {
    agent any
     environment {
             DOCKERHUB_CREDENTIALS = tool 'SonarQube Scanner'
        }
    stages {
        stage('SonarQube analysis') {
            steps {
                  withSonarQubeEnv('sq1') {
                        bat "${scannerHome}/bin/sonar-scanner" 
                }
            }
        }
    }
}
