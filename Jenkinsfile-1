pipeline {
    agent any
     environment {
             scannerHome = tool 'SonarQube'
        }
    stages {
        stage('SonarQube analysis') {
            steps {
                  withSonarQubeEnv('sq1') {
                    dir("client/"){
                     bat "${scannerHome}/bin/sonar-scanner.bat" 
                    }    
                }
            }
        }
    }
}
